{
  "parser": "babel-eslint",
  "extends": "eslint-config-honu-base",
  "env": {
    "node": true
  },
  "rules": {
    "flowtype/require-parameter-type": 0,
    "flowtype/require-return-type": 0,
    "flowtype/type-id-match": [ 2, "^([A-Z][A-z0-9]+)+Type$" ]
  },
  "plugins": [
    "flowtype"
  ]
}
